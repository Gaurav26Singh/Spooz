{
  "expo": {
    "name": "Spooz",
    "slug": "Spooz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/spooz.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/spoozlogoo.png",
      "resizeMode": "contain",
      "backgroundColor": "#b497d6"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/spooz.png",
        "backgroundColor": "#b497d6"
      },
      "package": "com.Spooz.Spooz"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5e9feef7-185a-4f4f-ab9c-9992448b6538"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/5e9feef7-185a-4f4f-ab9c-9992448b6538"
    }
  }
}
